import random
import urllib



def DownloadImage(url):
    name = random.randrange(1 , 10000)
    FullName = str(name) + ".jpg"
    urllib.urlretrieve(url, FullName)

print("Enter the url of the Image you want to Download below!")
enter = raw_input("")
DownloadImage(enter)

